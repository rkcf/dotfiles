Config { font = "Hack"
       , bgColor = "#021b21"
       , fgColor = "grey"
       , position = TopW R 100 
       , commands = [ Run Cpu ["-L","3","-H","50","--normal","#44b5b1","--high","c35159"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
					, Run Battery ["-t", "Bat: <left>%"] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% * %swap% | %battery%  <fc=#61778d>%date%</fc>"
       }
